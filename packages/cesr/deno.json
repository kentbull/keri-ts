{
  "name": "@keri-ts/cesr",
  "version": "0.2.2",
  "exports": {
    ".": "./mod.ts"
  },
  "imports": {
    "jsr:@std/assert": "jsr:@std/assert@^0.217.0",
    "@deno/dnt": "jsr:@deno/dnt@^0.42.3"
  },
  "tasks": {
    "test": "deno test",
    "annotate": "deno run --allow-read --allow-write src/annotate/cli-deno.ts",
    "version:generate": "deno run -A ../../scripts/generate_versions.ts --only cesr",
    "version:check": "deno run -A ../../scripts/generate_versions.ts --check --only cesr",
    "build:npm": "deno task version:generate && deno run -A scripts/build_npm.ts"
  }
}
